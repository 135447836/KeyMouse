{
    "profile":
    {
        "runOnStartUp": false,
        "backgroundColor": "#CCFFCC",
        "fontColor": "#000000",
        "fontSize": 10,
        "font": "Arial Rounded MT Bold"
    },
    "keybindings":
    {
        "toggleEnable": "alt+[",
        "scrollUp": "k",
        "scrollDown": "j",
        "selectMode": "alt+;",
        "escape": "esc",
        "fastSelectMode": "alt+j"
    }
}
